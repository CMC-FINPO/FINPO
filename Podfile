# platform :ios, '9.0'

target 'FINPO' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FINPO
  pod 'KakaoSDKCommon'  
  pod 'KakaoSDKAuth'  
  pod 'KakaoSDKUser' 
  pod 'KakaoSDKTalk'  
  pod 'KakaoSDKLink'  
  pod 'KakaoSDKTemplate'  
  pod 'RxKakaoSDKCommon'  # 필수 요소를 담은 공통 모듈
  pod 'RxKakaoSDKAuth'  # 카카오 로그인
  pod 'RxKakaoSDKUser'  # 사용자 관리
  pod 'RxKakaoSDKTalk'  # 친구, 메시지(카카오톡)
  pod 'RxKakaoSDKStory'  # 카카오스토리 
  pod 'RxKakaoSDKLink'  # 메시지(카카오톡 공유)
  pod 'Then'
  pod 'SnapKit', '~> 5.6.0'
  pod "GoogleSignIn",'6.2.2'
  pod "RxGesture"
  pod 'RxDataSources'
  pod 'Alamofire', '~> 5.1'
  
  target 'FINPOTests' do
    inherit! :search_paths
    pod 'RxTest'
    pod 'RxBlocking'
  end 

end
